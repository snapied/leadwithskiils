{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*.html",
      "destination": "/:path*",
      "permanent": true,
      "has": [
        {
          "type": "header",
          "key": "next-router-prefetch"
        }
      ]
    },
    {
      "source": "/404.html",
      "destination": "/404",
      "permanent": true
    }
  ]
}